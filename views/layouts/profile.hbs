{{#if publish}}
<div class="row d-flex justify-content-center align-items-center" style="height: 93%;">
        <div class="col-md-2 p-0 d-none d-xs-none d-md-none d-lg-block" >
        {{> profile/sideBar}}
    </div>
        <div class="col-md-10">
        {{> author/publish/new}}
    </div>
</div>

{{else}}
<div class="row" style="height: 93%;">
    <div class="col-md-2 p-0 d-none d-xs-none d-md-none d-lg-block" >
        {{> profile/sideBar}}
    </div>
    <div class="col-md-10">
        {{> profile/index}}
    </div>
</div>

{{/if}}